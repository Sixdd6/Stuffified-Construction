  <ThingDef ParentName="ResourceBase">
    <defName>Cinnabar</defName>
    <label>Cinnabar</label>
    <description>Because of its beauty and rarity, Cinnabar is often used for ornaments and decorations.</description>
    <graphicData>
      <texPath>Things/Item/Resource/PreciousStone</texPath>
      <color>(168, 79, 81)</color>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <useHitPoints>false</useHitPoints>
    <soundInteract>ChunkRock_Drop</soundInteract>
    <soundDrop>ChunkRock_Drop</soundDrop>
    <statBases>
      <MarketValue>5</MarketValue>
      <Mass>1.0</Mass>
      <SharpDamageMultiplier>0.8</SharpDamageMultiplier>
      <BluntDamageMultiplier>1.5</BluntDamageMultiplier>
    </statBases>
    <thingCategories>
      <li>RareStoneBlocks</li>
    </thingCategories>
    <deepCommonality>0.5</deepCommonality>
