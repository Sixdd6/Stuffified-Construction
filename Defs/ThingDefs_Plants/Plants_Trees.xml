<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>

	<ThingDef ParentName="TreeBase">
		<defName>PlantTreeSynthwood</defName>
		<label>Synthwood Tree</label>
		<description>A strong hardwood tree. Fast-growing and good for construction.</description>
		<graphicData>
			<texPath>Things/Plant/TreeOak</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<Beauty>2</Beauty>
		</statBases>
		<plant>
			<growDays>8</growDays>
			<leaflessGraphicPath>Things/Plant/TreeOak_Leafless</leaflessGraphicPath>
			<harvestWork>1000</harvestWork>
			<harvestYield>30</harvestYield>
			<harvestedThingDef>WoodLog</harvestedThingDef>
			<wildClusterSizeRange>
				<min>0</min>
				<max>0</max>
			</wildClusterSizeRange>
			<sowResearchPrerequisites>
				<li>Synthwood</li>
			</sowResearchPrerequisites>
		</plant>
	</ThingDef>

	<ThingDef ParentName="TreeBase">
		<defName>PlantTreeOak</defName>
		<label>Oak Tree</label>
