<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationReplace">
		<xpath>*/ThingDef[defName="TubeTelevision"]/label</xpath>
		<value>
			<label>Tube Television</label>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>*/ThingDef[defName="TubeTelevision"]/costList</xpath>
		<value>
			<Component>8</Component>
		</value>
	</Operation>

	<Operation Class="PatchOperationInsert">
		<xpath>*/ThingDef[defName="TubeTelevision"]</xpath>
		<value>
			<stuffCategories>
				<li>Metallic</li>
			</stuffCategories>
		</value>
	</Operation>

	<Operation Class="PatchOperationInsert">
		<xpath>*/ThingDef[defName="TubeTelevision"]</xpath>
		<value>
			<costStuffCount>100</costStuffCount>
		</value>
	</Operation>

</Patch>