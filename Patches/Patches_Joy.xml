<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationReplace">
		<xpath>*/ThingDef[defName="TubeTelevision"]/label</xpath>
		<value>
			<label>Tube Television</label>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>*/ThingDef[defName="TubeTelevision"]</xpath>
		<value>
			<stuffCategories>
				<li>Metallic</li>
			</stuffCategories>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>*/ThingDef[defName="TubeTelevision"]</xpath>
		<value>
			<costStuffCount>100</costStuffCount>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>*/ThingDef[defName="TubeTelevision"]/costList</xpath>
		<value>
			<costList>
				<Component>8</Component>
			</costList>
		</value>
	</Operation>

	<Operation Class="PatchOperationRemove">
		<xpath>*/ThingDef[defName="TubeTelevision"]/constructEffect</xpath>
	</Operation>

</Patch>